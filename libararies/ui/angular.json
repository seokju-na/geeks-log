{
  "$schema": "node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "ui": {
      "root": "",
      "sourceRoot": "src/",
      "projectType": "library",
      "prefix": "ui",
      "schematics": {
        "@schematics/angular:component": {
          "export": true,
          "style": "scss"
        },
        "@schematics/angular:directive": {
          "export": true
        }
      },
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "./node_modules/@angular/cdk/a11y-prebuilt.css",
              "./node_modules/@angular/cdk/overlay-prebuilt.css",
              "src/prebuilt.scss"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.lib.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/",
            "index": "services/dashboard/src/index.html",
            "main": "services/dashboard/src/main.ts",
            "polyfills": "services/dashboard/src/polyfills.ts",
            "tsConfig": "services/dashboard/src/tsconfig.app.json",
            "assets": [
              "services/dashboard/src/assets/"
            ],
            "styles": [
              "./node_modules/@angular/cdk/a11y-prebuilt.css",
              "./node_modules/@angular/cdk/overlay-prebuilt.css",
              "services/dashboard/src/styles.scss"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "services/dashboard/src/environments/environment.ts",
                  "with": "services/dashboard/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        }
      }
    }
  },
  "defaultProject": "ui"
}
