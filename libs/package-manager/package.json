{
  "name": "@geeks-log/package-manager",
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "esm/index.js",
  "bin": {
    "geeks-log-test": "bin/test.js",
    "geeks-log-deploy": "bin/deploy.js"
  },
  "scripts": {
    "build": "rm -rf dist/ && rm -rf esm/ && tsc -p tsconfig.build.json && tsc -p tsconfig.esm.json",
    "test": "tsc -d --noEmit && yarn lint",
    "lint": "eslint -c .eslintrc.js 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "deploy": "scripts/deploy.sh"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "conventional-commits-parser": "^3.0.1",
    "execa": "^1.0.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "nodegit": "^0.24.3",
    "read-pkg": "^5.2.0",
    "request": "^2.88.0",
    "write-pkg": "^4.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "@types/nodegit": "^0.24.8",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^5.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "typescript": "^3.5.3"
  }
}
